#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wangzy/git/smallCat/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wangzy/git/smallCat/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@microsoft+api-extractor@7.38.0_@types+node@20.8.4/node_modules/@microsoft/api-extractor/bin/api-extractor" "$@"
else
  exec node  "$basedir/../../../../../@microsoft+api-extractor@7.38.0_@types+node@20.8.4/node_modules/@microsoft/api-extractor/bin/api-extractor" "$@"
fi
